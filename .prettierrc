{
  "singleQuote": true,
  "semi": true,
  "trailingComma": "es5",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "bracketSameLine": false,
  "htmlWhitespaceSensitivity": "css",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^react$|^react-dom", "^@mui", "<THIRD_PARTY_MODULES>", "^src/.*$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
